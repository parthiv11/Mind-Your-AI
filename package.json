{
  "name": "mind-your-ai",
  "displayName": "Mind Your AI",
  "version": "0.0.1",
  "description": "Helping AI",
  "author": "Parthiv Makwana",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "mindsdb-js-sdk": "^2.2.2",
    "mysql": "^2.18.1",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.241",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28",
    "prettier": "3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6"
  },
  "manifest": {
    "commands": {
      "_execute_action": {
        "description": "Your AI Assistant powered by MindsDB. Answers your queries with Cmd+M.",
        "suggested_key": {
          "default": "Alt+A"
        }
      }
    },
    "host_permissions": [
      "https://*/*",
      "extension://*/*",
      "*://*/*"
    ],
    "permissions": [
      "tabs",
      "scripting",
      "contextMenus",
      "storage"
    ]
  }
}
